#ifndef PASS_H_
#define PASS_H_

#define READY  0x10
#define DONE   0x20
#define RIGHT  0x30
#define WRONG  0x40
#define OPEN   0x50
#define OPENED 0x60
#define CLOSE  0x70
#define FIRST 0x9
#define NOT_FIRST 0x8

uint16 PASSWARD_getPASSWARD(void);
uint16 PASSWARD_reEnterPASSWARD(void);
void PASSWARD_checkPassward(void);


#endif /* PASS_H_ */
